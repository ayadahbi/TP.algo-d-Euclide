#include <stdio.h>

int main() {
    int n, i, j, premier;

    printf("Donner un nombre n : ");
    scanf("%d", &n);

    printf("Les nombres premiers entre 1 et %d sont :\n", n);

    for (i = 2; i <= n; i++) {
        premier = 1; // On suppose que le nombre est premier

        // Vérifier si i a un diviseur
        for (j = 2; j * j <= i; j++) {
            if (i % j == 0) {
                premier = 0; // Donc il n’est pas premier
                break;
            }
        }

        // Si aucun diviseur n’a été trouvé
        if (premier == 1) {
            printf("%d ", i);
        }
    }

    return 0;
}
